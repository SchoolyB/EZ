{
  "name": "ez-language",
  "displayName": "EZ Language Support",
  "description": "Syntax highlighting for the EZ programming language",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "ez",
        "aliases": ["EZ", "ez"],
        "extensions": [".ez"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "ez",
        "scopeName": "source.ez",
        "path": "./syntaxes/ez.tmLanguage.json"
      }
    ]
  }
}
