/*
 * Error Test: E14002 - http-failed-request
 * Expected: "request failed" or "network" or "connection"
 */

import @http

do main() {
    // Use a URL that will fail to connect (invalid port on localhost)
    temp response, err = http.get("http://127.0.0.1:1")
    if err != nil {
        panic("${err}")
    }
}
