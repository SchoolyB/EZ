// E2042: @(strict) when statement cannot have a default case
import @std
using std

const Status enum {
    PENDING,
    ACTIVE
}

do main() {
    temp s = Status.PENDING

    @(strict)
    when s {
        is Status.PENDING { println("pending") }
        is Status.ACTIVE { println("active") }
        default { println("should not be here") }
    }
}
